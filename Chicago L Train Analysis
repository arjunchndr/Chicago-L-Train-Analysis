    "rides_by_year  = data_fe.groupby(data_fe['date'].dt.year)['rides'].mean()\n",
    "rides_by_month = data_fe.groupby(data_fe['date'].dt.month)['rides'].mean()\n",
    "fig, (ax1,ax2) = plt.subplots(1, 2, figsize=(14,4))\n",
