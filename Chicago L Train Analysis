    "dfmarginal = get_all_marginal_rides()\n",
    "top5 = ['Clark/Lake', 'Lake/State', 'Chicago/State', '95th/Dan Ryan', 'Belmont-North Main']\n",
    "plot_marginal(dfmarginal, top5 + ['Ashland-Lake'])"
   ]
